{
    "manifest_version": 3,
    "name": "Google Navigation",
    "description": "An Edge extension with a set of hotkeys to navigate through Google Search. ",
    "version": "0.0.4",
    "action": {
        "default_icon": "icon.png"
    },
    "content_scripts": [
        {
            "run_at": "document_idle",
            "matches": [
                "https://www.google.com/*",
                "http://www.google.com/*"
            ],
            "css": [
                "main.css"
            ],
            "js": [
                "main.js"
            ]
        }
    ]
}